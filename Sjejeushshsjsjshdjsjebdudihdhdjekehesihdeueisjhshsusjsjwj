-- New draggable Orion Lib script for hub creations!
local OrionLib = loadstring(game:HttpGet(('https://pastebin.com/raw/WRUyYTdY')))()

local Window = OrionLib:MakeWindow({
    Name = "PRIVATE HUB 1,2",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "Name" -- Put the name of your hub or script here!
})

print("Successfully executed.") -- Just a debug

-- ADMIN TAB
local TabAdmin = Window:MakeTab({
    Name = "admin",
    Icon = "rbxassetid://1",
    PremiumOnly = false
})

local playerName = game.Players.LocalPlayer.Name

TabAdmin:AddSection({ Name = "client admin" })

TabAdmin:AddButton({
    Name = "Welcome " .. playerName .. "! This is the best hub!",
    Callback = function() end
})

TabAdmin:AddButton({
    Name = "Mr bean admin",
    Callback = function() end
})

TabAdmin:AddButton({
    Name = "Nameless Admin",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ltseverydayyou/Nameless-Admin/main/Source.lua"))()
    end
})

TabAdmin:AddButton({
    Name = "infinite yield",
    Callback = function()
        loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Infinite%20Yield.txt"))()
    end
})

TabAdmin:AddButton({
    Name = "namelads admin",
    Callback = function()
        loadstring(game:HttpGet("https://github.com/FilteringEnabled/NamelessAdmin/blob/main/Source?raw=true"))()
    end
})

